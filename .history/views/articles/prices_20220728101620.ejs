<%- include('../partials/header') %>

<h1>You are now viewing the most up to date cryptocurrencies on the market</h1>

  <% cryptos.forEach(x => { %>
      <h5><%= x.symbol %></h5>
      <h5><%= x.name %></h5>
      <h5><%= x.price_usd %></h5>
      <h5><%= x.btcPrice %></h5>
      <h5><%= x.rank %></h5>
      <!-- <h5><%= x.coins.sparkline %></h5> -->
      <a href="<%= x.coins.coinrankingUrl %>">CoinRanking of <%= x.coins.name %></a>

   <% })  %>



