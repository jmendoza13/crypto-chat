<%- include('../partials/header') %>

<br>
<h1>You are now viewing the most up to date cryptocurrencies on the market</h1>

<table id="list">
  <thead>
    <tr>
      <th>id</th>
      <th>Symbol</th>
      <th>Name<br></th>
      <th>Rank</th>
      <th>USD Price</th>
      <th>BTC Price</th>
      <th>24H Change</th>
    </tr>
  </thead>
  <tbody>

  <% cryptos.forEach(x => { %>
      <tr>
      <td><%= x.id %></td>
      <td><%= x.symbol %></td>
      <td><%= x.name %></td>
      <td><%= x.rank %></td>
      <td><%= x.price_usd %></td>
      <td><%= x.price_btc %></td>
      <td><%= x.percent_change_24h %></td>
    </tr>
      <% })  %>
      
  </tbody>
  </table>


