<%- include('../partials/header') %>

<h1>You are now viewing the most up to date cryptocurrencies on the market</h1>

<table id="list">
  <thead>
    <tr>
      <th>Symbol</th>
      <th>Name<br></th>
      <th>Rank</th>
      <th>24H Change</th>
      <th>USD Price</th>
      <th>BTC Price</th>
    </tr>
  </thead>
  <tbody>
  <% cryptos.forEach(x => { %>
      <h5><%= x.symbol %></h5>
      <h5><%= x.name %></h5>
      <h5><%= x.rank %></h5>
      <h5><%= x.percent_change_24h %></h5>
      <h5><%= x.price_usd %></h5>
      <h5><%= x.btcPrice %></h5>

   <% })  %>



