<%- include('../partials/header') %>



<h2>Title</h2>
<p><%= article.title %></p>
<h2>Created At</h2>
<h2><%= article.createdAt %></h2>
<h2>Description</h2>
<h2><%= article.description %></h2>

<% articles.forEach(function(a) { %>
    <div class="card mt-4">
        <div class="card-body">
            <h4 class="card-title">
                <%=a.title %>
            </h4>
            <div class="card-date text-muted mb-2">
                <%=a.createdAt.toLocaleDateString() %>
            </div>
            <div class="card-text mb-2" card-body>
                <%=a.description %>
            </div>
        </div>
    </div>
    <% }); %>
</div>

<%- include('../partials/footer') %>
